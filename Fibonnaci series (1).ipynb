    "# Qno-fibonacci Series\n",
    "num=int(input(\"Enter a number: \"))\n",
    "a=-1\n",
    "b=1\n",
    "for i in range(num):\n",
    "    c=a+b\n",
    "    print(c,end=\" \")\n",
    "    a=b\n",
    "    b=c"
